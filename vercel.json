{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".output/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/_nuxt/(.*)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "dest": "/_nuxt/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
